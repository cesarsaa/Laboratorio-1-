x <- rexp(500,5)
media1 <- mean(x)
sd1 <- sd(x)
mediaMuestral=function(n){
muestra=rexp(500,5)
media=mean(muestra)
return(media)
}
m=1000
muchasMedias=replicate(m,mediaMuestral(n))
mean(muchasMedias)
sd(muchasMedias)
para5 <- muchasMedias[1:5]
para10 <- muchasMedias[1:10]
para20 <- muchasMedias[1:20]
para30 <- muchasMedias[1:30]
