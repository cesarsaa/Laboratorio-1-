m <-c(3700, 3600, 3500, 3400, 3300, 3200, 3100, 3000, 2900,2800,2700,2600,2500)
xbarra=3334  #Media de decision
sigma=400  #Desviacion de x
k=4   #tamaño de muestra
Proba=numeric(length(m))
for (i in 1:length(m)){
prob=pnorm((xbarra-m[i])/(sigma/sqrt(k)))
Proba[i]=prob
}
